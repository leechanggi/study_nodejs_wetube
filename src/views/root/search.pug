extends ../layout.pug 
include ../mixin/mixinVideo.pug

block content
  h4 Search
  form(method="get")
    input(type="text" name="keyword" value=keyword placeholder="Search by title")
    input(type="submit" value="Search Now")
  div
    each video in videos
      +mixinVideo(video)
    else
      div 검색결과가 없습니다.